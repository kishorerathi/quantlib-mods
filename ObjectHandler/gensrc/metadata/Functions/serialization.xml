    <Procedure name='ohObjectLoad'>
      <description>deserialize a list of objects</description>
      <alias>ObjectHandler::Repository::instance().loadObject</alias>
      <SupportedPlatforms>
        <SupportedPlatform name='Excel' />
      </SupportedPlatforms>
      <ParameterList>
        <Parameters>
          <Parameter name='idList'>
            <type>string</type>
            <tensorRank>vector</tensorRank>
            <description>list of objects to be deserialized</description>
          </Parameter>
          <Parameter name='path'>
            <type>string</type>
            <tensorRank>scalar</tensorRank>
            <description>file from which objects are to be deserialized</description>
          </Parameter>
        </Parameters>
      </ParameterList>
      <ReturnValue>
        <type>void</type>
        <tensorRank>scalar</tensorRank>
