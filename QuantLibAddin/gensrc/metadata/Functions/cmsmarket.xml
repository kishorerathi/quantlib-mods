      <ParameterList>
        <Parameters/>
      </ParameterList>
      <ReturnValue>
        <type>any</type>
        <tensorRank>matrix</tensorRank>
      </ReturnValue>
    </Member>
    
    <Member name='qlSimultaneousCalibrationBrowseCmsMarket' type='QuantLibAddin::SmileAndCmsCalibrationBySabr'>
      <description>return the market and implied spreads matrix</description>
      <libraryFunction>getCmsMarket</libraryFunction>
      <SupportedPlatforms>
        <SupportedPlatform name='Excel' calcInWizard='false' />
      </SupportedPlatforms>
      <ParameterList>
        <Parameters/>
