          <Parameter name='displacement' default='0.0'>
            <type>QuantLib::Spread</type>
            <tensorRank>scalar</tensorRank>
            <description>displacement</description>
          </Parameter>
        </Parameters>
      </ParameterList>
    </Constructor>

