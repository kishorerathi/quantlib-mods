            </ParameterList>
            <ReturnValue>
                <type>bool</type>
                <tensorRank>scalar</tensorRank>
            </ReturnValue>
        </Procedure>
        
        <Procedure name='qlrestoreNumericVector'>
            <description>Restore a vector of Real</description>
