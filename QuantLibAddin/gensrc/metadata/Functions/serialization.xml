    <Functions>
        <Procedure name='qlserializeNumericVector'>
            <description>Serialize a vector of Real</description>
            <alias>QuantLibAddin::serializeNumericVector</alias>
            <SupportedPlatforms>
                <SupportedPlatform name='Excel' calcInWizard='false' />
            </SupportedPlatforms>
            <ParameterList>
                <Parameters>
                    <Parameter name='vector' exampleValue ='1.0,10.1,3'>
                        <type>QuantLib::Real</type>
                        <tensorRank>vector</tensorRank>
                        <description>data to be serizalized</description>
                    </Parameter>
                    <Parameter name='path' exampleValue ='C:\Temp\test.xml'>
                        <type>string</type>
                        <tensorRank>scalar</tensorRank>
                        <description>path to the xml file</description>
                    </Parameter>
                </Parameters>
            </ParameterList>
            <ReturnValue>
                <type>bool</type>
                <tensorRank>scalar</tensorRank>
            </ReturnValue>
        </Procedure>

