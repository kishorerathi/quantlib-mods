    try {
%s        boost::shared_ptr<QuantLibAddin::%s> objectPointer =
            OH_GET_OBJECT(QuantLibAddin::%s, std::string(handle));
        if (!objectPointer)
            QL_FAIL("Error retrieving object " + std::string(handle));
        %s returnValue = %s(%s);
        %s;
        return SUCCESS;
    } catch (const std::exception &e) {
        ObjHandler::logMessage("%s Error: " + std::string(e.what()));
        result = 0;
        return FAIL;
    }
