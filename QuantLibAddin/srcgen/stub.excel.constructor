DLLEXPORT char* %s(
        char *handleStub,%s) {
    try {
        SET_SESSION_ID

%s        ObjHandler::obj_ptr objectPointer(new QuantLibAddin::%s(%s));
        std::string handleFull = ObjHandler::storeObject(handleStub, objectPointer);

        static char ret[XL_MAX_STR_LEN];
		ObjHandler::stringToChar(ret, handleFull);
        return ret;
    } catch (const std::exception &e) {
        ObjHandler::logMessage(std::string("ERROR: %s: ") + e.what(), 2);
        return 0;
    }
}

