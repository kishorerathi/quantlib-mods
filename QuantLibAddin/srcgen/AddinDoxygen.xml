<Addin name='Doxygen'>
<platformId>D</platformId>
<rootDirectory>../Docs/pages/</rootDirectory>

<Buffers>
<Buffer name='bufferEnumerations' fileName='stub.doxygen.enums'/>
<Buffer name='bufferCategories' fileName='stub.doxygen.categories'/>
<Buffer name='bufferHeader' fileName='stub.doxygen.header'/>
</Buffers>

<RuleGroups>

<RuleGroup name='functionDocs' indent='2'>
    <Rules>
    <Rule name='reformatType'>
        <SubRules>
        <SubRule>
            <tensorRanks>
                <tensorRank>vector</tensorRank>
            </tensorRanks>
            <replacements>
                <default>vector &lt; %s &gt;</default>
            </replacements>
        </SubRule>
        <SubRule>
            <tensorRanks>
                <tensorRank>matrix</tensorRank>
            </tensorRanks>
            <replacements>
                <default>vector &lt; vector &lt; %s &gt; &gt;</default>
            </replacements>
        </SubRule>
        </SubRules>
    </Rule>
    </Rules>
</RuleGroup>

<RuleGroup name='functionReturnCode'>
    <Rules>
    <Rule name='reformatType'>
        <SubRules>
        <SubRule>
            <tensorRanks>
                <tensorRank>vector</tensorRank>
            </tensorRanks>
            <replacements>
                <default>vector &lt; %s &gt;</default>
            </replacements>
        </SubRule>
        <SubRule>
            <tensorRanks>
                <tensorRank>matrix</tensorRank>
            </tensorRanks>
            <replacements>
                <default>vector &lt; vector &lt; %s &gt; &gt;</default>
            </replacements>
        </SubRule>
        </SubRules>
    </Rule>
    </Rules>
</RuleGroup>

</RuleGroups>

</Addin>

