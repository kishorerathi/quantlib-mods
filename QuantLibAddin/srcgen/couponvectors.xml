        <name>handle</name>
        <type>string</type>
        <tensorRank>scalar</tensorRank>
        <description>handle of coupon vector object</description>
    </parameter>
    <parameter>
        <name>termStructureID</name>
        <type>string</type>
        <tensorRank>scalar</tensorRank>
        <description>discounting term structure</description>
    </parameter>    
    <parameter ignore='true'>
        <name>trigger</name>
        <type>any</type>
        <tensorRank>scalar</tensorRank>
        <description>dependency tracking trigger</description>
    </parameter>
</parameters>
<returnValue>
    <type>double</type>
    <tensorRank>scalar</tensorRank>
    <description>BPS</description>
</returnValue>
</function>

</functions>
