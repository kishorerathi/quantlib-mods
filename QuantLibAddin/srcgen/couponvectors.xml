<category>
<headerOnly>false</headerOnly>
<description>functions to construct QuantLib coupon vector objects</description>
<displayName>Coupon Vectors</displayName>
<functions>

<function>
<name>qlFixedRateCouponVector</name>
<description>construct and return a handle to a fixed rate coupon vector</description>
<constructor>true</constructor>
<libraryFunction>FixedRateCouponVector</libraryFunction>
<functionCategory>QuantLib</functionCategory>
<parameters>
    <parameter>
        <name>scheduleID</name>
        <type>string</type>
        <tensorRank>scalar</tensorRank>
        <description>schedule</description>
    </parameter>
    <parameter>
        <name>conventionID</name>
        <type>string</type>
        <tensorRank>scalar</tensorRank>
        <description>payment adjustment</description>
    </parameter>
    <parameter>
        <name>nominals</name>
        <type>double</type>
        <tensorRank>vector</tensorRank>
        <description>coupon nominals</description>
    </parameter>
    <parameter>
        <name>couponRates</name>
        <type>double</type>
        <tensorRank>vector</tensorRank>
        <description>coupon fixed rates</description>
    </parameter>
	<parameter>
        <name>dayCountID</name>
        <type>string</type>
        <tensorRank>scalar</tensorRank>
        <description>day counter</description>
    </parameter>
</parameters>
<returnValue>
    <type>string</type>
    <tensorRank>scalar</tensorRank>
    <description>handle of newly created object</description>
</returnValue>
</function>

<function>
<name>qlFloatingRateCouponVector</name>
<description>construct and return a handle to a floating rate coupon vector</description>
<constructor>true</constructor>
<libraryFunction>FloatingRateCouponVector</libraryFunction>
<functionCategory>QuantLib</functionCategory>
<parameters>
    <parameter>
        <name>scheduleID</name>
        <type>string</type>
        <tensorRank>scalar</tensorRank>
        <description>schedule</description>
    </parameter>
    <parameter>
        <name>nominals</name>
        <type>double</type>
        <tensorRank>vector</tensorRank>
        <description>coupon nominals</description>
    </parameter>
    <parameter>
        <name>indexID</name>
        <type>string</type>
        <tensorRank>scalar</tensorRank>
        <description>underlying index</description>
    </parameter>
    <parameter default='0'>
        <name>spreads</name>
        <type>double</type>
        <tensorRank>vector</tensorRank>
        <description>floating rate spreads</description>
    </parameter>
</parameters>
<returnValue>
    <type>string</type>
    <tensorRank>scalar</tensorRank>
    <description>handle of newly created object</description>
</returnValue>
</function>

<function>
<name>qlGetLeg</name>
<description>return coupon details</description>
<constructor>false</constructor>
<libraryFunction>getLeg</libraryFunction>
<functionCategory>QuantLib</functionCategory>
<parameters>
    <parameter class='CouponVector'>
        <name>handle</name>
        <type>string</type>
        <tensorRank>scalar</tensorRank>
        <description>handle of coupon vector object</description>
    </parameter>
    <parameter ignore='true'>
        <name>trigger</name>
        <type>any</type>
        <tensorRank>scalar</tensorRank>
        <description>dependency tracking trigger</description>
    </parameter>
</parameters>
<returnValue>
    <type>double</type>
    <tensorRank>matrix</tensorRank>
    <description>coupon details</description>
</returnValue>
</function>

<function>
<name>qlGetBPS</name>
<description>basis point sensitivity</description>
