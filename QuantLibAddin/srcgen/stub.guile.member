    try {
%s        boost::shared_ptr< QuantLibAddin::%s > objectPointer =
            OH_GET_OBJECT(QuantLibAddin::%s, handle);
        if (!objectPointer)
            QL_FAIL("Error retrieving object " + std::string(handle));
        %s returnValue = %s(%s);
        return %s;
    } catch (const std::exception &e) {
        ObjHandler::logMessage(std::string("%s Error: ") + e.what(), 2);
        return SCM_EOL;
    }
}

