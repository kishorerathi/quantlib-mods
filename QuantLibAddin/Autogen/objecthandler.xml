<constructor>false</constructor>
<qlfunction></qlfunction>
<platforms>SGO</platforms>
<parameters></parameters>
<returnval>
    <type>string</type>
    <tensorrank>scalar</tensorrank>
    <desc>ObjectHandler version number</desc>
</returnval>
</function>

