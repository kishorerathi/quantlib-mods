<?xml version='1.0'?>
<!-- DOCTYPE functiongroup SYSTEM 'functiongroup.dtd' -->
<functiongroup>
<headeronly>false</headeronly>
<desc>functions to construct QuantLib Xibor objects</desc>
<displayname>Indices</displayname>
<functions>

<function>
<name>QL_XIBOR</name>
<codename>qlXibor</codename>
<desc>construct and return a handle to a Xibor Index</desc>
<constructor>true</constructor>
<qlfunction>Xibor</qlfunction>
<parameters>
    <param>
        <name>IndexName</name>
        <type>string</type>
        <tensorrank>scalar</tensorrank>
        <desc>evaluation date</desc>
    </param>
    <param>
        <name>Currency</name>
        <type>string</type>
        <tensorrank>scalar</tensorrank>
        <desc>Index Currency</desc>
    </param>
    <param>
        <name>tenor</name>
        <type>long</type>
        <tensorrank>scalar</tensorrank>
        <desc>the tenor of this index</desc>
    </param>
    <param>
        <name>timeUnits</name>
        <type>string</type>
        <tensorrank>scalar</tensorrank>
        <desc>time units applying to above tenor</desc>
    </param>
	<param>
        <name>calendar</name>
        <type>string</type>
        <tensorrank>scalar</tensorrank>
        <desc>calendar (e.g. TARGET)</desc>
    </param>
    <param>
        <name>BDayConvention</name>
        <type>string</type>
        <tensorrank>scalar</tensorrank>
        <desc>business day convention (e.g. ModifiedFollowing)</desc>
    </param>
    <param>
        <name>dayCounter</name>
        <type>string</type>
        <tensorrank>scalar</tensorrank>
        <desc>day counter (e.g. Actual360)</desc>
    </param>
    <param>
        <name>fixingDays</name>
        <type>long</type>
        <tensorrank>scalar</tensorrank>
        <desc>fixing days (e.g. 2)</desc>
    </param>
    <param class='YieldTermStructure'>
        <name>ForwardCurve</name>
        <type>string</type>
        <tensorrank>scalar</tensorrank>
        <desc>handle of the Yield Curve used for forecasting</desc>
    </param>
        <param>
        <name>dates</name>
        <type>long</type>
        <tensorrank>vector</tensorrank>
        <desc>dates</desc>
    </param>
    <param>
        <name>fixings</name>
        <type>double</type>
        <tensorrank>vector</tensorrank>
        <desc>fixings</desc>
    </param>
</parameters>
<returnval>
    <type>string</type>
    <tensorrank>scalar</tensorrank>
    <desc>handle of newly created object</desc>
</returnval>
</function>

</functions>
</functiongroup>

