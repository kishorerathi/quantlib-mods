<?xml version='1.0'?>
<!-- DOCTYPE functiongroup SYSTEM 'functiongroup.dtd' -->
<functiongroup>
<headeronly>false</headeronly>
<desc>functions to construct QuantLib schedule objects</desc>
<displayname>Schedules</displayname>
<functions>

<function>
<name>qlSchedule</name>
<desc>construct and return a handle to a schedule object</desc>
<constructor>true</constructor>
<qlfunction>Schedule</qlfunction>
<parameters>
    <param>
        <name>calendar</name>
        <type>string</type>
        <tensorrank>scalar</tensorrank>
        <desc>calendar</desc>
    </param>
    <param>
        <name>startDate</name>
        <type>long</type>
        <tensorrank>scalar</tensorrank>
        <desc>start date</desc>
    </param>
    <param>
        <name>endDate</name>
        <type>long</type>
        <tensorrank>scalar</tensorrank>
        <desc>end date</desc>
    </param>
    <param>
        <name>frequency</name>
        <type>string</type>
        <tensorrank>scalar</tensorrank>
        <desc>frequency</desc>
    </param>
    <param>
        <name>convention</name>
        <type>string</type>
        <tensorrank>scalar</tensorrank>
        <desc>business day convention</desc>
    </param>
    <param default='0'>
        <name>startFromEnd</name>
        <type>bool</type>
        <tensorrank>scalar</tensorrank>
        <desc>start from end</desc>
    </param>
    <param default='0'>
        <name>longFinal</name>
        <type>bool</type>
        <tensorrank>scalar</tensorrank>
        <desc>long final</desc>
    </param>
</parameters>
<returnval>
    <type>string</type>
    <tensorrank>scalar</tensorrank>
    <desc>handle of newly created object</desc>
</returnval>
</function>

</functions>
</functiongroup>

