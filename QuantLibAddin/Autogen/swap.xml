<?xml version='1.0'?>
<!-- DOCTYPE functiongroup SYSTEM 'functiongroup.dtd' -->
<functiongroup>
<headeronly>false</headeronly>
<desc>functions to construct QuantLib swap objects</desc>
<displayname>Swap</displayname>
<functions>

<function>
<name>QL_SWAP</name>
<codename>qlSwap</codename>
<desc>construct and return a handle to a swap object</desc>
<constructor>true</constructor>
<qlfunction>Swap</qlfunction>
<parameters>
    <param>
        <name>paidLegID</name>
        <type>string</type>
        <tensorrank>scalar</tensorrank>
        <desc>handle to paid leg</desc>
    </param>
    <param>
        <name>recvLegID</name>
        <type>string</type>
        <tensorrank>scalar</tensorrank>
        <desc>handle to receveid leg</desc>
    </param>
    <param>
        <name>termStructureID</name>
        <type>string</type>
        <tensorrank>scalar</tensorrank>
        <desc>handle to discounting term structure</desc>
    </param>
</parameters>
<returnval>
    <type>string</type>
    <tensorrank>scalar</tensorrank>
    <desc>handle of newly created object</desc>
</returnval>
</function>

</functions>
</functiongroup>

