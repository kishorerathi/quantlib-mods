<?xml version='1.0'?>
<!-- DOCTYPE functiongroup SYSTEM 'functiongroup.dtd' -->
<functiongroup>
<headeronly>false</headeronly>
<desc>functions to construct QuantLib short-rate model objects</desc>
<displayname>Short Rate Models</displayname>
<functions>

<function>
<name>qlHullWhite</name>
<desc>construct and return a handle to a Hull-White short-rate model object</desc>
<constructor>true</constructor>
<qlfunction>HullWhite</qlfunction>
<parameters>
    <param class='YieldTermStructure'>
        <name>handleTermStructure</name>
        <type>string</type>
        <tensorrank>scalar</tensorrank>
        <desc>handle to a term structure object</desc>
    </param>
    <param>
        <name>a</name>
        <type>double</type>
        <tensorrank>scalar</tensorrank>
        <desc>a</desc>
    </param>
    <param>
        <name>sigma</name>
        <type>double</type>
        <tensorrank>scalar</tensorrank>
        <desc>volatility</desc>
    </param>
</parameters>
<returnval>
    <type>string</type>
    <tensorrank>scalar</tensorrank>
    <desc>handle of newly created object</desc>
</returnval>
</function>

<function>
<name>qlVasicek</name>
<desc>construct and return a Vasicek short-rate model object</desc>
<constructor>true</constructor>
<qlfunction>Vasicek</qlfunction>
<parameters>
    <param>
        <name>a</name>
        <type>double</type>
        <tensorrank>scalar</tensorrank>
        <desc>mean reverting speed</desc>
    </param>
    <param>
        <name>b</name>
        <type>double</type>
        <tensorrank>scalar</tensorrank>
        <desc>short-rate limit value</desc>
    </param>
    <param>
        <name>lambda</name>
        <type>double</type>
        <tensorrank>scalar</tensorrank>
        <desc>risk premium</desc>
    </param>
    <param>
        <name>sigma</name>
        <type>double</type>
        <tensorrank>scalar</tensorrank>
        <desc>volatility</desc>
    </param>
</parameters>
<returnval>
    <type>string</type>
    <tensorrank>scalar</tensorrank>
    <desc>handle of newly created object</desc>
</returnval>
</function>

</functions>
</functiongroup>

