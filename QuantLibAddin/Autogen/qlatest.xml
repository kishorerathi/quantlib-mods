<?xml version="1.0"?>
<!-- DOCTYPE functionlist SYSTEM "functionlist.dtd" -->
<functionlist>
<headeronly>false</headeronly>
<desc>functions to perform tests of internal QuantLibAddin functionality</desc>
<displayname>QuantLibAddin Tests</displayname>

<function>
<name>QL_TEST</name>
<codename>qlTest</codename>
<desc>construct and return a handle to a QlaTest object</desc>
<constructor>true</constructor>
<qlfunction>QlaTest</qlfunction>
<parameters>
    <param>
        <name>scalarLong</name>
        <type>long</type>
        <tensorrank>scalar</tensorrank>
        <desc>scalar long</desc>
    </param>
    <param>
        <name>vectorLong</name>
        <type>long</type>
        <tensorrank>vector</tensorrank>
        <desc>vector long</desc>
    </param>
    <param>
        <name>matrixLong</name>
        <type>long</type>
        <tensorrank>matrix</tensorrank>
        <desc>matrix long</desc>
    </param>
    <param>
        <name>scalarDouble</name>
        <type>double</type>
        <tensorrank>scalar</tensorrank>
        <desc>scalar double</desc>
    </param>
    <param>
        <name>vectorDouble</name>
        <type>double</type>
        <tensorrank>vector</tensorrank>
        <desc>vector double</desc>
    </param>
    <param>
        <name>matrixDouble</name>
        <type>double</type>
        <tensorrank>matrix</tensorrank>
        <desc>matrix double</desc>
    </param>
    <param>
        <name>scalarBool</name>
        <type>bool</type>
        <tensorrank>scalar</tensorrank>
        <desc>scalar bool</desc>
    </param>
    <param>
        <name>vectorBool</name>
        <type>bool</type>
        <tensorrank>vector</tensorrank>
        <desc>vector bool</desc>
    </param>
    <param>
        <name>matrixBool</name>
        <type>bool</type>
        <tensorrank>matrix</tensorrank>
        <desc>matrix bool</desc>
    </param>
    <param>
        <name>scalarString</name>
        <type>string</type>
        <tensorrank>scalar</tensorrank>
        <desc>scalar string</desc>
    </param>
    <param>
        <name>vectorString</name>
        <type>string</type>
        <tensorrank>vector</tensorrank>
        <desc>vector string</desc>
    </param>
    <param>
        <name>matrixString</name>
        <type>string</type>
        <tensorrank>matrix</tensorrank>
        <desc>matrix string</desc>
    </param>
    <param>
        <name>scalarAny</name>
        <type>any</type>
        <tensorrank>scalar</tensorrank>
        <desc>scalar any</desc>
    </param>
    <param>
        <name>vectorAny</name>
        <type>any</type>
        <tensorrank>vector</tensorrank>
        <desc>vector any</desc>
    </param>
    <param>
        <name>matrixAny</name>
        <type>any</type>
        <tensorrank>matrix</tensorrank>
        <desc>matrix any</desc>
    </param>
</parameters>

<returnval>
    <type>property</type>
    <tensorrank>vector</tensorrank>
    <desc>vector of properties describing QlaTest object</desc>
</returnval>
</function>

