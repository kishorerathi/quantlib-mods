<name>QL_TEST_SCALAR_LONG</name>
<codename>qlTestScalarLong</codename>
<desc>test processing of scalar long values</desc>
<constructor>false</constructor>
<qlfunction>echoScalarLong</qlfunction>
<parameters>
    <param>
        <name>handle</name>
        <type class="QlaTest">string</type>
        <tensorrank>scalar</tensorrank>
        <desc>handle of a QlaTest object</desc>
    </param>
    <param>
        <name>scalarLong</name>
        <type>long</type>
        <tensorrank>scalar</tensorrank>
        <desc>scalar long</desc>
    </param>
</parameters>
<returnval>
    <type>long</type>
    <tensorrank>scalar</tensorrank>
    <desc>echo back the input value</desc>
</returnval>
</function>

